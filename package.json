{
  "name": "socialviz",
  "version": "1.0.0",
  "description": "Social Graph visualizer",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=logo.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . SocialViz --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . SocialViz --overwrite --asar=true --platform=linux --arch=x64 --icon=logo.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/euler16/SocialViz.git"
  },
  "author": "Nilay Shrivastava",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/euler16/SocialViz/issues"
  },
  "homepage": "https://github.com/euler16/SocialViz#readme",
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-packager": "^12.2.0"
  },
  "dependencies": {
    "save-svg-as-png": "^1.4.6",
    "simple-datatables": "^1.7.5"
  }
}
